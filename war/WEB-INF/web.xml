<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
     version="2.5">
    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>flashcards.LoginServlet</servlet-class>
    </servlet>
    <servlet>
	    <servlet-name>Csv</servlet-name>
	    <servlet-class>flashcards.CsvServlet</servlet-class>
    </servlet>
    <servlet>
	    <servlet-name>Home</servlet-name>
	    <servlet-class>flashcards.HomePageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddDeck</servlet-name>
        <servlet-class>flashcards.AddDeckServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditDeck</servlet-name>
        <servlet-class>flashcards.EditDeckServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteDeck</servlet-name>
        <servlet-class>flashcards.DeleteDeckServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCard</servlet-name>
        <servlet-class>flashcards.AddCardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCard</servlet-name>
        <servlet-class>flashcards.DeleteCardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RenameDeck</servlet-name>
        <servlet-class>flashcards.RenameDeckServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Csv</servlet-name>
	    <url-pattern>/csv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
	    <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
	    <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddDeck</servlet-name>
        <url-pattern>/addDeck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditDeck</servlet-name>
        <url-pattern>/editDeck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteDeck</servlet-name>
        <url-pattern>/deleteDeck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCard</servlet-name>
        <url-pattern>/addCard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCard</servlet-name>
        <url-pattern>/deleteCard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RenameDeck</servlet-name>
        <url-pattern>/renameDeck</url-pattern>
    </servlet-mapping>
   <welcome-file-list>
        <welcome-file>login</welcome-file>
    </welcome-file-list>
</web-app>
